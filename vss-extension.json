{
    "manifestVersion": 1,
    "id": "scribe-solution-deployment",
    "publisher": "ToluAdepoju",
    "version": "1.1.24",
    "name": "Scribe Solution Deployment",
    "description": "Deployment of Solutions for Tibco Scribe ",
    "public": false,
    "categories": ["Azure Repos"],
    "baseUri": "https://localhost:44300",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "contributions": [
        {
            "id": "scribe-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildAndReleaseTask"
            }
        }
    ],
    "files": [
        {
            "path": "buildAndReleaseTask",
            "addressable": true
        }
    ]
}