{
    "manifestVersion": 1,
    "id": "scribe-solution-deployment",
    "publisher": "ToluAdepoju",
    "version": "1.2.10",
    "name": "Scribe Solution Deployment",
    "description": "Facilitates deployment of scribe solutions.",
    "public": true,
    "icons": {
        "default": "images/extension-icon.png"
    },
    "tags": [
        "scribe",
        "ci",
        "continuous integration",
        "Dynamics CE"
    ],
    "galleryFlags": [
        "public" 
    ],
    "categories": ["Azure Repos"],
    "content": {
        "details": {
            "path": "overview.md"
        },
        "license": {
            "path": "LICENSE.md"
        }
    },
    "links": {
        "learn": {
            "uri": "https://marketplace.visualstudio.com/items?itemName=ToluAdepoju.scribe-solution-deployment"
        },
        "home": {
            "uri": "https://marketplace.visualstudio.com/items?itemName=ToluAdepoju.scribe-solution-deployment"
        },
        "getstarted": {
            "uri": "https://marketplace.visualstudio.com/items?itemName=ToluAdepoju.scribe-solution-deployment"
        },
        "repository": {
            "uri": "https://github.com/wase932/ScribeADOExtension"
        },
        "issues": {
            "uri": "https://github.com/wase932/ScribeADOExtension/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/wase932/"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "contributions": [
        {
            "id": "scribe-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildAndReleaseTask"
            }
        }
    ],
    "files": [
        {
            "path": "buildAndReleaseTask",
            "addressable": true
        }
    ]
}